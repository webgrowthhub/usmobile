<script>
   $(function(){
      
      $('#customPhonenno').usPhoneFormat({
    format:'(xxx) xxx-xxxx'
  });

  $('.customPhonenno').text(function(i, text) {
    return text.replace(/(\d{3})(\d{3})(\d{4})/, '($1) $2-$3');
});

$("#post-like-btn").on("click",function(){
   var company_no=$(this).data("id");
   $.ajax({
               type: "POST",
               url: "/mark_number",
               data: {'phone_no': company_no,'mark' : 'safe'},
               success: function(r){
                if(r == 'true'){
                  location.reload();
                }
               
               },
               error: function (e) {
                  //   console.log("some error", e);
               }
            });
})

$("#post-dislike-btn").on("click",function(){
   var company_no=$(this).data("id");
   $.ajax({
               type: "POST",
               url: "/mark_number",
               data: {'phone_no': company_no,'mark' : 'unsafe'},
               success: function(r){
                if(r == 'true'){
                  location.reload();
                }
               
               },
               error: function (e) {
                  //   console.log("some error", e);
               }
            });
})

  $('#customPhonenno').on("keyup",function(){
     var inputNo=$("#customPhonenno").val();
var cleanNumber =  inputNo.replace(/[^\d.]/g, ''); 
if(cleanNumber.toString().length >= 5){
    $.ajax({
               type: "POST",
               url: "/get_results",
               data: {'phone_no': cleanNumber},
               success: function(r){
                  $(".curentResult").show();
                  $("#result_ui").html('');
                 
                  if(r == 'false'){
                     $("#result_ui").append('<li class="li_number" >No record Found</li>');
        
                  }else{
                     r.forEach(function (item) { 
                     
                     $("#result_ui").append('<li class="li_number" data-name="'+item.companynumber+'" data-attr="http://159.89.238.39/phone-number='+item.companynumber+'"><a href="http://159.89.238.39/phone-number/'+item.companynumber+'">'+item.companynumber+'</a></li>');
        }); 
                  }
          
               // location.reload();
               },
               error: function (e) {
                  //   console.log("some error", e);
               }
            });
}
        
  })


 


   })
</script>


